#include<iostream>
using namespace std;

class Node {
	public:
		int data;
		Node* next;
		
		Node(int d) {
			this -> data = d;
			this -> next = NULL;
		}
};

void insertAtHead(Node* &head, int d) {
	Node* temp = new Node(d);
	
	if(head==NULL) {
		head=temp;
	}
	else {
		temp->next = head;
		head=temp;
	}
}

void printList(Node* head) {
	Node* temp = head;
	
	while(temp!=NULL) {
		cout<<temp->data<<" ";
		temp=temp->next;
	}
}

int main() {
	Node* head = NULL;
	
	int n,d;
	cin>>n;
	
	for(int i=0;i<n;i++) {
		cin>>d;
		insertAtHead(head, d);
	}
	cout<<endl;
	printList(head);
	return 0;
}
