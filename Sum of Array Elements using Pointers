#include<iostream>
using namespaces std;

int sum(int *a, int n) {
  int total = 0;
  
  for(int i =0;i<n;i++) {
    total += *(a+i);
  }
  
  return total;
}

int main() {
  int n;
  cin>>n;
  
  int *a  new int[n];
  
  for(int i=0; i<n;i++) {
    cin>>a[i];
  }
  
  int result = sum(a,n);
  cout<<result;

  delete[] a;
  
  return 0;
}
