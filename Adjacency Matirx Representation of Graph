#include<iostream>
using namespace std;

int main() {
	int n, edges;
	cin>>n;
	cin>>edges;
	
	//initizlize adjecency matrix with 0
	int adj[100][100]={0}; //assuming max 100 vertices
	
	cout<<"Enter (u, v) edges: ";
	for(int i=0;i<edges;i++) {
		int u, v;
		cin>>u>>v;
		
		adj[u][v]=1;
		adj[v][u]=1; //for undirected graph only
	}
	
	cout<<"Adjecency Matrix : ";
	for(int i=0;i<n;i++) {
		for(int j=0;j<n;j++) {
			cout<<adj[i][j]<<" ";
		}
		cout<<endl;
	}
	return 0;
}
